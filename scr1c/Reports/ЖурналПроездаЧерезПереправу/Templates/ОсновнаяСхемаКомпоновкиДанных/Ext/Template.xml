		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	РегистрацияПроезда.Ссылка КАК Ссылка,
	РегистрацияПроезда.Номер КАК Номер,
	РегистрацияПроезда.Дата КАК Дата,
	РегистрацияПроезда.Контрагент КАК Контрагент,
	РегистрацияПроезда.ТС.Наименование КАК ТСИмя,
	РегистрацияПроезда.ТС.Код КАК ТСКод,
	РегистрацияПроезда.ТС КАК ТС,
	РегистрацияПроезда.Масса КАК Масса,
	РегистрацияПроезда.Тариф КАК Тариф,
	РегистрацияПроезда.Сумма КАК Сумма,
	РегистрацияПроезда.Ответственный КАК Ответственный
ИЗ
	Документ.РегистрацияПроезда КАК РегистрацияПроезда
ГДЕ
	РегистрацияПроезда.Дата &gt;= &amp;Дата1
	И РегистрацияПроезда.Дата &lt; &amp;Дата2
	И РегистрацияПроезда.ПометкаУдаления = false 
	и (РегистрацияПроезда.Контрагент = &amp;КА или &amp;БезКА = Истина)

УПОРЯДОЧИТЬ ПО
	Дата</query>
